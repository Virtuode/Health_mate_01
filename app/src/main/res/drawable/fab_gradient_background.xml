<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- White stroke -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#FFFFFF"/>
            <size android:width="68dp" android:height="68dp"/>
        </shape>
    </item>
    <!-- Gradient background with padding to create stroke effect -->
    <item android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp">
        <shape android:shape="oval">
            <gradient
                android:startColor="#4FD1C5"
                android:endColor="#63B3ED"
                android:angle="45"/>
        </shape>
    </item>
</layer-list>
